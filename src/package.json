{
  "name": "golobe-demo",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.7.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i7en0v91/travel-agency-website.git"
  },
  "author": "i7en0v91",
  "license": "MIT",
  "homepage": "https://github.com/i7en0v91/travel-agency-website#readme",
  "bugs": {
    "url": "https://github.com/i7en0v91/travel-agency-website/issues"
  },
  "scripts": {
    "prebuild": "npm run prisma:generate-migration-scripts --workspace=@golobe-demo/backend && npm run cms-startup --workspace=@golobe-demo/backend && npx uglifyjs public/js/page-load.js  --compress --mangle -o public/js/page-load.min.js && npx copyfiles -E -f ./packages/shared/locales/* assets/locales",
    "build": "npm run build:checks && cross-env NODE_OPTIONS='--max-old-space-size=4096' nuxt build",
    "build:checks": "npx jiti ./scripts/build-checks.ts",
    "dev": "npm run prebuild && npx copyfiles -E -f ./packages/shared/locales/* .nuxt/locales && cross-env NEXTAUTH_URL=http://127.0.0.1:3000/api/auth nuxt dev",
    "generate": "nuxt generate",
    "preview": " npx copyfiles -E -f ./packages/shared/locales/* .output/locales && cross-env NEXTAUTH_URL=http://127.0.0.1:3000/api/auth env-cmd -f ./packages/backend/.env nuxt preview",
    "postinstall": "nuxt prepare",
    "apptests": "cross-env VITE_VITEST=true NODE_OPTIONS='--max-old-space-size=4096 --disable-warning=DEP0166 --disable-warning=DEP0155' NEXTAUTH_URL=http://127.0.0.1:43321/api/auth env-cmd -f ./packages/backend/.env npx vitest run --dir=./tests",
    "test": "npm run test:e2e",
    "test:e2e": "npm run apptests -- -t e2e:",
    "test:og-image-static": "npm run apptests -- -t og:image",
    "check-types": "npx nuxi typecheck",
    "lint": "eslint . --ignore-pattern \"public/js/*.js\"",
    "lint:fix": "eslint . --fix --ignore-pattern \"public/js/*.js\"",
    "sourcemap-error": "npx jiti ./scripts/sourcemap-error.ts",
    "html-validator": "npx html-validator --continue",
    "unlighthouse-guest": "cd ./tests/unlighthouse && cross-env UNLIGHTHOUSE_MODE=guest npx unlighthouse",
    "unlighthouse-authenticated": "cd ./tests/unlighthouse && cross-env UNLIGHTHOUSE_MODE=authenticated npx unlighthouse",
    "quickstart:build": "env-cmd -f ./packages/backend/.env cross-env NODE_ENV=production VITE_QUICKSTART=1 NODE_OPTIONS='--disable-warning=DEP0166 --disable-warning=DEP0155' npm run build",
    "quickstart:run": "cross-env NODE_ENV=production VITE_QUICKSTART=1 npm run preview",
    "quickstart": "npm run quickstart:prepare --workspace=@golobe-demo/backend && npm run quickstart:build && npm run quickstart:run"
  },
  "optionalDependencies": {
    "vue": ">=2.7.0"
  },
  "devDependencies": {
    "@css-inline/css-inline": "^0.14.1",
    "@iconify-json/cil": "^1.2.0",
    "@iconify-json/gridicons": "^1.2.1",
    "@iconify-json/grommet-icons": "^1.2.0",
    "@iconify-json/heroicons-solid": "^1.2.0",
    "@iconify-json/ion": "^1.2.1",
    "@iconify-json/line-md": "^1.2.2",
    "@iconify-json/logos": "^1.2.3",
    "@iconify-json/material-symbols": "^1.2.2",
    "@iconify-json/material-symbols-light": "^1.2.3",
    "@iconify-json/mdi": "^1.2.0",
    "@iconify-json/mdi-light": "^1.2.1",
    "@iconify-json/ph": "^1.2.0",
    "@nuxt/content": "^2.13.4",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxt/ui": "^2.18.6",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "^2.0.0-rc.11",
    "@sidebase/nuxt-auth": "^0.7.2",
    "@tiptap/extension-placeholder": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@types/file-saver": "^2.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.199",
    "@types/lodash-es": "^4.17.12",
    "@types/mime": "^4.0.0",
    "@types/on-headers": "^1.0.3",
    "@unlighthouse/core": "^0.13.1",
    "@vue/test-utils": "^2.4.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dayjs-nuxt": "^2.1.9",
    "env-cmd": "^10.1.0",
    "happy-dom": "^13.2.1",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "~3.12.4",
    "nuxt-og-image": "~3.0.0-rc.64",
    "nuxt-svgo": "^4.0.6",
    "nuxt-tiptap-editor": "^1.1.2",
    "playwright-core": "^1.45.3",
    "sass": "^1.77.8",
    "source-map": "^0.7.4",
    "typescript": "^5.2.2",
    "uglify-js": "^3.18.0",
    "unlighthouse": "^0.13.1",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@golobe-demo/backend": ">=0.1",
    "@golobe-demo/shared": ">=0.1",
    "@nuxt/image": "^1.7.0",
    "@pinia/nuxt": "^0.5.1",
    "@vue-final-modal/nuxt": "^1.0.3",
    "@vueform/slider": "^2.1.10",
    "axios": "^1.7.2",
    "c12": "^1.11.1",
    "cropperjs": "^1.6.2",
    "decimal.js": "^10.4.3",
    "destr": "^2.0.3",
    "file-saver": "^2.0.5",
    "isbot": "^5.1.11",
    "mime": "^4.0.4",
    "next-auth": "~4.21.1",
    "on-headers": "^1.0.2",
    "pinia": "^2.1.7",
    "scss-reset": "^1.4.2",
    "v-calendar": "^3.1.2",
    "vue-final-modal": "^4.5.4",
    "vue-yandex-maps": "^2.1.4",
    "vue3-recaptcha2": "^1.8.0",
    "yup": "^1.4.0"
  },
  "workspaces": [
    "packages/backend",
    "packages/shared"
  ]
}
