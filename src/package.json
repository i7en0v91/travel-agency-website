{
  "name": "golobe-demo",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "npx uglifyjs public/js/page-load.js  --compress --mangle -o public/js/page-load.min.js && npx copyfiles -E -f node_modules/blueimp-canvas-to-blob/js/* public/js",
    "build": "nuxt build && npx copyfiles -E -f locales/* .output/locales",
    "dev": "npm run prebuild && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "cross-env VITE_VITEST=true npx vitest run",
    "check-types": "npx nuxi typecheck",
    "lint": "npx eslint './**/*.{js,ts,vue}'",
    "sourcemap-error": "npx ts-node scripts/sourcemap-error.ts",
    "html-validator": "npx html-validator --continue",
    "unlighthouse-guest": "cross-env UNLIGHTHOUSE_MODE=guest npx unlighthouse",
    "unlighthouse-authenticated": "cross-env UNLIGHTHOUSE_MODE=authenticated npx unlighthouse",
    "quickstart": "npx prisma db push --schema=./prisma/schema.prisma.sqlite && npx prisma generate --schema=./prisma/schema.prisma.sqlite && cross-env VITE_QUICKSTART=1 npm run build && cross-env VITE_QUICKSTART=1 npm run preview"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/i18n": "^8.0.0",
    "@nuxtseo/module": "^2.0.0-beta.55",
    "@samk-dev/nuxt-vcalendar": "^1.0.2",
    "@sidebase/nuxt-auth": "^0.6.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.199",
    "@types/nodemailer": "^6.4.14",
    "@types/on-headers": "^1.0.3",
    "@types/traverse": "^0.6.33",
    "@unlighthouse/nuxt": "^0.10.4",
    "@vue/test-utils": "^2.4.3",
    "chalk": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "happy-dom": "^13.2.1",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.9.1",
    "playwright-core": "^1.41.1",
    "prisma": "^5.8.0",
    "sass": "^1.69.3",
    "source-map": "^0.7.4",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4",
    "vitest": "^1.2.1",
    "vue": "^3.4.8",
    "vue-router": "^4.2.5",
    "w3c-html-validator": "^1.5.1"
  },
  "dependencies": {
    "@nuxt/image": "^1.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.8.0",
    "@types/mime": "^3.0.3",
    "@vue-final-modal/nuxt": "^1.0.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.6.2",
    "blueimp-canvas-to-blob": "^3.29.0",
    "cropperjs": "^1.6.1",
    "dayjs-nuxt": "^2.1.9",
    "destr": "^2.0.1",
    "floating-vue": "^5.0.3",
    "js-base64": "^3.7.5",
    "juice": "^10.0.0",
    "mime": "^3.0.0",
    "next-auth": "^4.21.1",
    "nodemailer": "^6.9.7",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-swiper": "^1.2.2",
    "on-headers": "^1.0.2",
    "pinia": "^2.1.7",
    "scss-reset": "^1.4.2",
    "slugify": "^1.6.6",
    "traverse": "^0.6.7",
    "typed-inject": "^4.0.0",
    "vue-final-modal": "^4.4.6",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vue3-recaptcha2": "^1.8.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yup": "^1.3.2"
  }
}
