{
  "name": "golobe-demo",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.2.4",
  "scripts": {
    "prebuild": "npx uglifyjs public/js/page-load.js  --compress --mangle -o public/js/page-load.min.js && npx copyfiles -E -f node_modules/blueimp-canvas-to-blob/js/* public/js",
    "build": "nuxt build && npx copyfiles -E -f locales/* .output/locales",
    "dev": "npm run prebuild && cross-env NEXTAUTH_URL=http://127.0.0.1:3000/api/auth nuxt dev",
    "generate": "nuxt generate",
    "preview": "cross-env NEXTAUTH_URL=http://127.0.0.1:3000/api/auth nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "cross-env VITE_VITEST=true NODE_OPTIONS='--max-old-space-size=4096' NEXTAUTH_URL=http://127.0.0.1:43321/api/auth npx vitest run -t e2e:",
    "og-image-generate": "cross-env VITE_VITEST=true NODE_OPTIONS='--max-old-space-size=4096' NEXTAUTH_URL=http://127.0.0.1:43321/api/auth npx vitest run -t og:image",
    "check-types": "npx nuxi typecheck",
    "lint": "eslint . --ignore-pattern \"public/js/*.js\"",
    "lint:fix": "eslint . --fix --ignore-pattern \"public/js/*.js\"",
    "sourcemap-error": "npx ts-node --cwd ./scripts sourcemap-error.ts",
    "html-validator": "npx html-validator --continue",
    "unlighthouse-guest": "cross-env UNLIGHTHOUSE_MODE=guest npx unlighthouse",
    "unlighthouse-authenticated": "cross-env UNLIGHTHOUSE_MODE=authenticated npx unlighthouse",
    "quickstart": "npx prisma db push --schema=./prisma/schema.prisma.sqlite && npx prisma generate --schema=./prisma/schema.prisma.sqlite && cross-env VITE_QUICKSTART=1 NODE_OPTIONS='--disable-warning=DEP0166 --disable-warning=DEP0155' npm run build && cross-env VITE_QUICKSTART=1 NEXTAUTH_URL=http://127.0.0.1:3000/api/auth npm run preview"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^0.3.8",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "^2.0.0-rc.8",
    "@samk-dev/nuxt-vcalendar": "^1.0.3",
    "@sidebase/nuxt-auth": "^0.7.2",
    "@tiptap/extension-placeholder": "^2.3.0",
    "@tiptap/extension-underline": "^2.3.0",
    "@types/blob-stream": "^0.1.33",
    "@types/file-saver": "^2.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.199",
    "@types/lodash-es": "^4.17.12",
    "@types/nodemailer": "^6.4.14",
    "@types/on-headers": "^1.0.3",
    "@types/pdfkit": "^0.13.4",
    "@types/sanitize-html": "^2.11.0",
    "@types/traverse": "^0.6.33",
    "@unlighthouse/nuxt": "^0.10.4",
    "@vue/test-utils": "^2.4.3",
    "chalk": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dayjs-nuxt": "^2.1.9",
    "happy-dom": "^13.2.1",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.11.2",
    "nuxt-og-image": "^3.0.0-rc.52",
    "nuxt-tiptap-editor": "^1.1.1",
    "playwright-core": "^1.41.1",
    "prisma": "^5.13.0",
    "sass": "^1.69.3",
    "source-map": "^0.7.4",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4",
    "unlighthouse": "^0.10.4",
    "vitest": "^1.2.1",
    "vue": "^3.4.8",
    "vue-router": "^4.2.5",
    "w3c-html-validator": "^1.5.1"
  },
  "dependencies": {
    "@nuxt/image": "^1.4.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.13.0",
    "@types/mime": "^3.0.3",
    "@vue-final-modal/nuxt": "^1.0.3",
    "@vueform/slider": "^2.1.10",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.6.8",
    "blob-stream": "^0.1.3",
    "blueimp-canvas-to-blob": "^3.29.0",
    "cropperjs": "^1.6.2",
    "decimal.js": "^10.4.3",
    "destr": "^2.0.3",
    "file-saver": "^2.0.5",
    "floating-vue": "^5.2.2",
    "isbot": "^5.1.4",
    "js-base64": "^3.7.7",
    "juice": "^10.0.0",
    "mime": "^3.0.0",
    "next-auth": "~4.21.1",
    "nodemailer": "^6.9.7",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-swiper": "^1.2.2",
    "on-headers": "^1.0.2",
    "pdfkit": "^0.15.0",
    "pinia": "^2.1.7",
    "sanitize-html": "^2.13.0",
    "scss-reset": "^1.4.2",
    "slugify": "^1.6.6",
    "traverse": "^0.6.7",
    "typed-inject": "^4.0.0",
    "vue-final-modal": "^4.5.4",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-yandex-maps": "^2.1.2",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vue3-recaptcha2": "^1.8.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.3.2"
  }
}
