{
  "name": "golobe-demo",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.7.0",
  "scripts": {
    "prebuild": "npm run backend-prisma:generate-migration-scripts && npx uglifyjs public/js/page-load.js  --compress --mangle -o public/js/page-load.min.js",
    "build": "npm run build:checks && nuxt build && npx copyfiles -E -f locales/* .output/locales",
    "build:checks": "npx jiti ./scripts/build-checks.ts",
    "backend-prisma:generate-migration-scripts": "npx jiti ./scripts/generate-modified-utc-migration.ts && npx jiti ./scripts/generate-acsys-views-migration.ts",
    "backend-prisma:generate-client": "npx prisma generate --schema=server/backend/prisma/mysql/schema.prisma",
    "backend-prisma:migrate-reset": "npx prisma migrate reset --force --schema=server/backend/prisma/mysql/schema.prisma",
    "dev": "npm run prebuild && cross-env NEXTAUTH_URL=http://127.0.0.1:3000/api/auth nuxt dev",
    "generate": "nuxt generate",
    "preview": "cross-env NEXTAUTH_URL=http://127.0.0.1:3000/api/auth nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "cross-env VITE_VITEST=true NODE_OPTIONS='--max-old-space-size=4096 --disable-warning=DEP0166 --disable-warning=DEP0155' NEXTAUTH_URL=http://127.0.0.1:43321/api/auth npx vitest run",
    "test:e2e": "npm run test -- -t e2e:",
    "test:unit": "npm run test -- -t unit:",
    "test:og-image-static": "npm run test -- -t og:image",
    "check-types": "npx nuxi typecheck",
    "lint": "eslint . --ignore-pattern \"public/js/*.js\"",
    "lint:fix": "eslint . --fix --ignore-pattern \"public/js/*.js\"",
    "sourcemap-error": "npx jiti ./scripts/sourcemap-error.ts",
    "html-validator": "npx html-validator --continue",
    "unlighthouse-guest": "cd ./tests/unlighthouse && cross-env UNLIGHTHOUSE_MODE=guest npx unlighthouse",
    "unlighthouse-authenticated": "cd ./tests/unlighthouse && cross-env UNLIGHTHOUSE_MODE=authenticated npx unlighthouse",
    "quickstart": "cross-env VITE_QUICKSTART=1 npm run backend-prisma:generate-migration-scripts && npx prisma migrate reset --schema=server/backend/prisma/sqlite/schema.prisma --force && npx prisma generate --schema=server/backend/prisma/sqlite/schema.prisma && cross-env NODE_ENV=production VITE_QUICKSTART=1 NODE_OPTIONS='--disable-warning=DEP0166 --disable-warning=DEP0155' npm run build && cross-env NODE_ENV=production VITE_QUICKSTART=1 NEXTAUTH_URL=http://127.0.0.1:3000/api/auth npm run preview"
  },
  "optionalDependencies": {
    "vue": ">=2.7.0"
  },
  "devDependencies": {
    "@css-inline/css-inline": "^0.14.1",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "^2.0.0-rc.11",
    "@samk-dev/nuxt-vcalendar": "^1.0.3",
    "@sidebase/nuxt-auth": "^0.7.2",
    "@tiptap/extension-placeholder": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@types/file-saver": "^2.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.199",
    "@types/lodash-es": "^4.17.12",
    "@types/mime": "^4.0.0",
    "@types/minipass": "^3.3.5",
    "@types/nodemailer": "^6.4.14",
    "@types/on-headers": "^1.0.3",
    "@types/pdfkit": "^0.13.4",
    "@types/sanitize-html": "^2.11.0",
    "@types/traverse": "^0.6.33",
    "@unlighthouse/core": "^0.13.1",
    "@vue/test-utils": "^2.4.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dayjs-nuxt": "^2.1.9",
    "happy-dom": "^13.2.1",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.12.4",
    "nuxt-og-image": "^3.0.0-rc.64",
    "nuxt-tiptap-editor": "^1.1.2",
    "playwright-core": "^1.45.3",
    "prisma": "~5.14.0",
    "sass": "^1.77.8",
    "source-map": "^0.7.4",
    "typescript": "^5.2.2",
    "uglify-js": "^3.18.0",
    "unlighthouse": "^0.13.1",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@nuxt/image": "^1.7.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "~5.14.0",
    "@vue-final-modal/nuxt": "^1.0.3",
    "@vueform/slider": "^2.1.10",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.7.2",
    "c12": "^1.11.1",
    "cropperjs": "^1.6.2",
    "decimal.js": "^10.4.3",
    "destr": "^2.0.3",
    "file-saver": "^2.0.5",
    "floating-vue": "^5.2.2",
    "isbot": "^5.1.11",
    "juice": "^10.0.0",
    "mime": "^4.0.4",
    "minipass": "^7.1.2",
    "next-auth": "~4.21.1",
    "nodemailer": "^6.9.7",
    "nuxt-swiper": "^1.2.2",
    "on-headers": "^1.0.2",
    "pdfkit": "^0.15.0",
    "pinia": "^2.1.7",
    "sanitize-html": "^2.13.0",
    "scss-reset": "^1.4.2",
    "slugify": "^1.6.6",
    "traverse": "^0.6.9",
    "typed-inject": "^4.0.0",
    "vue-final-modal": "^4.5.4",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-yandex-maps": "^2.1.4",
    "vue3-perfect-scrollbar": "^2.0.0",
    "vue3-recaptcha2": "^1.8.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.4.0"
  }
}
